{
    "name": "lucasgiovanny/laravel-erede",
    "description": "Laravel Service Provider to use eRede PHP SDK",
    "keywords": ["laravel", "erede", "rede", "payment", "pagamento"],
    "authors": [
        {
            "name": "Lucas Giovanny",
            "email": "lucasgiovanny@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
